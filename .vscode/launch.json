{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Electron: Main",
      "protocol": "inspector",
      "env": {
        "NODE_ENV": "production"
      },
      "cwd": "${workspaceRoot}",
      "sourceMaps": true,
      "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron",
      "windows": {
        "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron.cmd"
      },
      "program": "${workspaceRoot}/app/main/dist/main.prod.js",
    }
  ]
}
